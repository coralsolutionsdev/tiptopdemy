{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.2.3",
        "ext-json": "*",
        "arrilot/laravel-widgets": "^3.12",
        "biscolab/laravel-recaptcha": "*",
        "bnbwebexpertise/laravel-attachments": "1.0.*",
        "bumbummen99/shoppingcart": "^2.10",
        "cviebrock/eloquent-sluggable": "^8.0",
        "cybercog/laravel-love": "^8.9",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "facade/ignition": "^2.3.6",
        "filp/whoops": "^2.7",
        "guzzlehttp/guzzle": "^7.0.1",
        "haruncpi/laravel-id-generator": "^1.0",
        "intervention/image": "^2.5",
        "james-heinrich/getid3": "^1.9",
        "laravel/framework": "^8.0",
        "laravel/nexmo-notification-channel": "^2.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "monolog/monolog": "^2.04",
        "nunomaduro/collision": "^5.0",
        "pion/laravel-chunk-upload": "^1.4",
        "pusher/pusher-php-server": "^4.0",
        "rajurayhan/larastreamer": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^2.3",
        "santigarcor/laratrust": "^7.2",
        "shivella/laravel-bitly": "^1.1",
        "spatie/laravel-medialibrary": "9.0.0",
        "spatie/laravel-tags": "*",
        "symfony/translation": "^6.3",
        "vinkla/hashids": "*",
        "webpatser/laravel-countries": "1.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel-shift/blueprint": "^1.8",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Http/helpers.php"]


    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
